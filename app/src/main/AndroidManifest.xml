<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sudhaker_ftc_test">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/login" />
        <activity
            android:name=".SignUpActivity"
            android:label="@string/sign_up" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ForgotPasswordActivity"
            android:label="@string/forgotPass" />
        <activity
            android:name=".AppointmentListActivity"
            android:label="@string/title_activity_appointment_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddAppointmentActivity"
            android:label="@string/add_appointment" />
        <activity
            android:name=".ContactList"
            android:label="Contact List" />

        <receiver android:name=".receiver.AlarmReceiver"></receiver>
    </application>

</manifest>